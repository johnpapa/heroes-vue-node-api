    "body-parser": "^1.18.2",
    "bulma": "^0.7.2",
    "cors": "^2.8.5",
    "express": "^4.16.3",
    "vue": "^2.5.21",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.1.1",
